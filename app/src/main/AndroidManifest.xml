<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.kevinjanvier.slauversion1"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    
        <activity
            android:name=".Main.MainNavigationDrawer"
            android:label="@string/title_activity_main_navigation_drawer"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Signup.RegisterPage">
        </activity>
        <activity
            android:name=".Main.welcomeScreen"
            android:theme="@style/AppTheme_Splash">
        </activity>
        <activity android:name=".Login.Profile">
        </activity>
    </application>

</manifest>
